{
  "openapi": "3.0.3",
  "info": {
    "title": "Plagium API",
    "description": "Plagium REST API v3 — plagiarism detection and AI-generated content detection service.",
    "version": "3.5.6",
    "contact": {
      "name": "Septet Systems",
      "email": "info@septetsystems.com",
      "url": "https://www.septetsystems.com"
    }
  },
  "servers": [
    {
      "url": "https://api.plagium.com",
      "description": "Production"
    }
  ],
  "security": [
    {
      "apiKey": []
    }
  ],
  "paths": {
    "/300/document/create": {
      "post": {
        "operationId": "createDocument",
        "summary": "Create a new document",
        "description": "Creates a new plagiarism check document from text or a URL.\nThe document is automatically queued for processing after creation.\n",
        "tags": [
          "Documents"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AuthenticatedRequest",
                "x-data-schema": {
                  "$ref": "#/components/schemas/CreateDocumentData"
                }
              },
              "example": {
                "key": "YOUR_API_KEY",
                "data": {
                  "text": "Text to check for plagiarism.",
                  "type": "deep",
                  "source": [
                    "web"
                  ],
                  "title": "My document"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Document created successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "obj": {
                      "$ref": "#/components/schemas/Document"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          }
        }
      }
    },
    "/300/document/upload": {
      "post": {
        "operationId": "uploadDocument",
        "summary": "Upload a file and create a document",
        "description": "Creates a new plagiarism check by uploading a file.\nAccepted formats: DOC, DOCX, PDF, TXT.\nUses multipart/form-data encoding.\n",
        "tags": [
          "Documents"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "required": [
                  "key",
                  "file"
                ],
                "properties": {
                  "key": {
                    "type": "string",
                    "description": "API key for authentication"
                  },
                  "password": {
                    "type": "string",
                    "description": "API password (if enabled on the account)"
                  },
                  "data": {
                    "type": "string",
                    "description": "JSON-encoded document options (type, source, title, webhook, etc.)"
                  },
                  "file": {
                    "type": "string",
                    "format": "binary",
                    "description": "File to upload (DOC, DOCX, PDF, or TXT)"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Document created from uploaded file",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "obj": {
                      "$ref": "#/components/schemas/Document"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          }
        }
      }
    },
    "/300/document/get": {
      "post": {
        "operationId": "getDocument",
        "summary": "Get a document",
        "description": "Retrieves a document by its ID.",
        "tags": [
          "Documents"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AuthenticatedRequest",
                "x-data-schema": {
                  "$ref": "#/components/schemas/DocumentIdData"
                }
              },
              "example": {
                "key": "YOUR_API_KEY",
                "data": {
                  "id": "document:xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Document retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "obj": {
                      "$ref": "#/components/schemas/Document"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          }
        }
      }
    },
    "/300/document/list": {
      "post": {
        "operationId": "listDocuments",
        "summary": "List documents",
        "description": "Lists documents owned by the authenticated user.\nSupports filtering by status, keywords, and date range.\nReturns up to 100 documents per page.\n",
        "tags": [
          "Documents"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AuthenticatedRequest",
                "x-data-schema": {
                  "$ref": "#/components/schemas/ListDocumentsData"
                }
              },
              "example": {
                "key": "YOUR_API_KEY",
                "data": {
                  "status": "ready",
                  "from": "2024-01-01",
                  "to": "2024-12-31"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Documents listed successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "results": {
                      "$ref": "#/components/schemas/PaginatedResults"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          }
        }
      }
    },
    "/300/document/results": {
      "post": {
        "operationId": "getDocumentResults",
        "summary": "Get plagiarism detection results",
        "description": "Retrieves the plagiarism detection results for a document.\nReturns the document metadata along with matched sources, scores, and comparisons.\n",
        "tags": [
          "Documents"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AuthenticatedRequest",
                "x-data-schema": {
                  "$ref": "#/components/schemas/DocumentIdData"
                }
              },
              "example": {
                "key": "YOUR_API_KEY",
                "data": {
                  "id": "document:xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Results retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "obj": {
                      "$ref": "#/components/schemas/Document"
                    },
                    "results": {
                      "type": "object",
                      "properties": {
                        "count": {
                          "type": "integer",
                          "description": "Number of results returned"
                        },
                        "total": {
                          "type": "integer",
                          "description": "Total number of results"
                        },
                        "objs": {
                          "type": "array",
                          "items": {
                            "$ref": "#/components/schemas/PlagiarismResult"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          }
        }
      }
    },
    "/300/document/embedded-report-url": {
      "post": {
        "operationId": "getEmbeddedReportUrl",
        "summary": "Get embedded report URL",
        "description": "Generates signed URLs for viewing plagiarism reports.\nReturns both a standalone report URL and an embeddable iframe URL.\n",
        "tags": [
          "Documents"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AuthenticatedRequest",
                "x-data-schema": {
                  "$ref": "#/components/schemas/DocumentIdData"
                }
              },
              "example": {
                "key": "YOUR_API_KEY",
                "data": {
                  "id": "document:xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Report URLs generated",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "documentID": {
                      "type": "string",
                      "description": "Original document ID"
                    },
                    "signedID": {
                      "type": "string",
                      "description": "Signed (encoded) document ID"
                    },
                    "reportUrl": {
                      "type": "string",
                      "format": "uri",
                      "description": "Full-page report URL on plagium.com"
                    },
                    "embeddedReportUrl": {
                      "type": "string",
                      "format": "uri",
                      "description": "Embeddable iframe report URL"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          }
        }
      }
    },
    "/300/account/remaining_credits": {
      "post": {
        "operationId": "getAccountRemainingCredits",
        "summary": "Get remaining credits",
        "description": "Returns the remaining credits for the authenticated account.",
        "tags": [
          "Account"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AuthenticatedRequest"
              },
              "example": {
                "key": "YOUR_API_KEY"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Remaining credits",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "remaining_credits": {
                      "$ref": "#/components/schemas/Credits"
                    }
                  }
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          }
        }
      }
    },
    "/300/sub_account/create": {
      "post": {
        "operationId": "createSubAccount",
        "summary": "Create a sub-account",
        "description": "Creates a sub-account under the current account.\nRequires institution-level access and a valid product code.\n",
        "tags": [
          "Sub-Accounts"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AuthenticatedRequest",
                "x-data-schema": {
                  "$ref": "#/components/schemas/CreateSubAccountData"
                }
              },
              "example": {
                "key": "YOUR_API_KEY",
                "data": {
                  "product": "PRODUCT_CODE",
                  "email": "user@example.com",
                  "username": "newuser",
                  "firstname": "John",
                  "lastname": "Doe",
                  "api": true
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Sub-account created",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "obj": {
                      "$ref": "#/components/schemas/MemberSummary"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          }
        }
      }
    },
    "/300/sub_account/list": {
      "post": {
        "operationId": "listSubAccounts",
        "summary": "List sub-accounts",
        "description": "Lists all sub-accounts owned by the authenticated account. Returns up to 100 per page.",
        "tags": [
          "Sub-Accounts"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AuthenticatedRequest",
                "x-data-schema": {
                  "$ref": "#/components/schemas/PaginationData"
                }
              },
              "example": {
                "key": "YOUR_API_KEY"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Sub-accounts listed",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "results": {
                      "$ref": "#/components/schemas/PaginatedResults"
                    }
                  }
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          }
        }
      }
    },
    "/300/sub_account/add_credits": {
      "post": {
        "operationId": "addCreditsToSubAccount",
        "summary": "Add credits to a sub-account",
        "description": "Purchases credits using the product definition and optionally transfers them to a sub-account.\nRequires institution-level access and a valid product code.\nIf `user_id` is provided in data, credits are transferred to that sub-account.\n",
        "tags": [
          "Sub-Accounts"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AuthenticatedRequest",
                "x-data-schema": {
                  "$ref": "#/components/schemas/AddCreditsData"
                }
              },
              "example": {
                "key": "YOUR_API_KEY",
                "data": {
                  "product": "PRODUCT_CODE",
                  "user_id": "member:xxxxxxxx"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Credits added and transferred",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "transaction": {
                      "$ref": "#/components/schemas/TransactionSummary"
                    }
                  }
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "500": {
            "description": "Internal server error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        }
      }
    },
    "/300/sub_account/remaining_credits": {
      "post": {
        "operationId": "getSubAccountRemainingCredits",
        "summary": "Get sub-account remaining credits",
        "description": "Returns the remaining credits for a sub-account.\nRequires institution-level access and a valid product code.\n",
        "tags": [
          "Sub-Accounts"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AuthenticatedRequest",
                "x-data-schema": {
                  "$ref": "#/components/schemas/SubAccountCreditsData"
                }
              },
              "example": {
                "key": "YOUR_API_KEY",
                "data": {
                  "user_id": "member:xxxxxxxx",
                  "product": "PRODUCT_CODE"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Remaining credits for sub-account",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "remaining_credits": {
                      "$ref": "#/components/schemas/Credits"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          }
        }
      }
    },
    "/300/search/quick": {
      "post": {
        "operationId": "quickSearch",
        "summary": "Quick plagiarism search",
        "description": "Performs a quick web search for matching content.\nRate-limited by a daily free search quota defined by the product.\nText is truncated to the product's maximum character limit.\n",
        "tags": [
          "Search"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AuthenticatedRequest",
                "x-data-schema": {
                  "$ref": "#/components/schemas/QuickSearchData"
                }
              },
              "example": {
                "key": "YOUR_API_KEY",
                "data": {
                  "text": "Text to search for plagiarism.",
                  "product": "PRODUCT_CODE"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Search results",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "text": {
                      "type": "string",
                      "description": "The text that was searched (may be truncated)"
                    },
                    "results": {
                      "type": "array",
                      "description": "Matching web results",
                      "items": {
                        "type": "object"
                      }
                    },
                    "remaining": {
                      "type": "integer",
                      "nullable": true,
                      "description": "Number of free searches remaining today (null for internal API)"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "apiKey": {
        "type": "apiKey",
        "in": "body",
        "name": "key",
        "description": "API key passed in the request body `key` field"
      }
    },
    "responses": {
      "Unauthorized": {
        "description": "Authentication failed — invalid or missing API key",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/Error"
            },
            "example": {
              "error": {
                "code": 401,
                "message": "Unable to load api user information"
              }
            }
          }
        }
      },
      "BadRequest": {
        "description": "Invalid request parameters",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/Error"
            }
          }
        }
      }
    },
    "schemas": {
      "AuthenticatedRequest": {
        "type": "object",
        "required": [
          "key"
        ],
        "properties": {
          "key": {
            "type": "string",
            "description": "API key or member key for authentication"
          },
          "password": {
            "type": "string",
            "description": "API password (required only if password is enabled on the account)"
          },
          "data": {
            "type": "object",
            "description": "Endpoint-specific parameters"
          }
        }
      },
      "CreateDocumentData": {
        "type": "object",
        "description": "Parameters for creating a document",
        "properties": {
          "text": {
            "type": "string",
            "description": "Text content to check for plagiarism (required if no URL or file)"
          },
          "url": {
            "type": "string",
            "format": "uri",
            "description": "URL to fetch content from (must start with `http`)"
          },
          "type": {
            "type": "string",
            "enum": [
              "quick",
              "deep"
            ],
            "default": "deep",
            "description": "Search depth"
          },
          "source": {
            "type": "array",
            "items": {
              "type": "string",
              "enum": [
                "web"
              ]
            },
            "default": [
              "web"
            ],
            "description": "Sources to search against"
          },
          "title": {
            "type": "string",
            "description": "Document title (auto-generated from text if omitted)"
          },
          "read": {
            "type": "string",
            "enum": [
              "public",
              "member",
              "group",
              "child",
              "private"
            ],
            "default": "private",
            "description": "Privacy level"
          },
          "import_id": {
            "type": "string",
            "description": "External import identifier"
          },
          "min_relation": {
            "type": "number",
            "description": "Minimum similarity relation threshold"
          },
          "min_rank": {
            "type": "number",
            "description": "Minimum similarity rank threshold"
          },
          "webhook": {
            "oneOf": [
              {
                "type": "string",
                "description": "Webhook URL (legacy format)"
              },
              {
                "type": "object",
                "description": "Webhook configuration",
                "properties": {
                  "url": {
                    "type": "string",
                    "format": "uri"
                  },
                  "format": {
                    "type": "string"
                  },
                  "metadata": {
                    "type": "object"
                  }
                }
              }
            ]
          },
          "webhook_format": {
            "type": "string",
            "description": "Webhook response format (legacy, use `webhook.format` instead)"
          },
          "webhook_metadata": {
            "type": "object",
            "description": "Webhook metadata (legacy, use `webhook.metadata` instead)"
          },
          "user_id": {
            "type": "string",
            "description": "Run on behalf of this user (requires parent or super admin access)"
          }
        }
      },
      "DocumentIdData": {
        "type": "object",
        "required": [
          "id"
        ],
        "properties": {
          "id": {
            "type": "string",
            "description": "Document ID"
          },
          "user_id": {
            "type": "string",
            "description": "Run on behalf of this user (requires parent or super admin access)"
          }
        }
      },
      "ListDocumentsData": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string",
            "description": "Filter by document status"
          },
          "keywords": {
            "type": "string",
            "description": "Search in title, description, author, keywords, and filename"
          },
          "from": {
            "type": "string",
            "format": "date",
            "description": "Filter documents created after this date"
          },
          "to": {
            "type": "string",
            "format": "date",
            "description": "Filter documents created before this date"
          },
          "pagination": {
            "type": "object",
            "description": "Pagination cursor from a previous response"
          },
          "user_id": {
            "type": "string",
            "description": "Run on behalf of this user"
          }
        }
      },
      "CreateSubAccountData": {
        "type": "object",
        "required": [
          "product"
        ],
        "properties": {
          "product": {
            "type": "string",
            "description": "Product code (required)"
          },
          "email": {
            "type": "string",
            "format": "email",
            "description": "Sub-account email (auto-generated UUID if omitted)"
          },
          "username": {
            "type": "string",
            "description": "Sub-account username (defaults to email if omitted)"
          },
          "password": {
            "type": "string",
            "description": "Sub-account password (auto-generated UUID if omitted)"
          },
          "firstname": {
            "type": "string",
            "description": "First name"
          },
          "lastname": {
            "type": "string",
            "description": "Last name"
          },
          "api": {
            "type": "boolean",
            "description": "If true, generates an API key for the sub-account"
          }
        }
      },
      "PaginationData": {
        "type": "object",
        "properties": {
          "pagination": {
            "type": "object",
            "description": "Pagination cursor from a previous response"
          }
        }
      },
      "AddCreditsData": {
        "type": "object",
        "required": [
          "product"
        ],
        "properties": {
          "product": {
            "type": "string",
            "description": "Product code defining the credit amount and currency"
          },
          "user_id": {
            "type": "string",
            "description": "Sub-account ID to transfer credits to"
          }
        }
      },
